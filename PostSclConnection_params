INSERT INTO connection_params (
                                  id,
                                  server_url,
                                  port,
                                  username,
                                  password,
                                  token,
                                  expiration_date,
                                  created_at,
                                  updated_at,
                                  is_active,
                                  description,
                                  fName
                              )
                              VALUES 
                                  (
                                  ?, --id
                                  ?, --server_url
                                  ?, --port
                                  ?, --username
                                  ?, --password
                                  ?, --token
                                  ?, --expiration_date
                                  ?, --created_at
                                  ?, --updated_at
                                  ?, --is_active
                                  ?, --description
                                  ?, --fName
                              );
